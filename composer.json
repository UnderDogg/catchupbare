{
  "name": "catchupcrm/catchupbase",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "doctrine/dbal": "^2.6@dev",
    "laravelcollective/html": "^5.1",
    "yaap/theme": "^2.0",
    "barryvdh/laravel-ide-helper": "^2.0@dev",
    "barryvdh/laravel-debugbar": "^2.0@dev",
    "laracasts/flash": "^1.3",
    "zizaco/entrust": "dev-laravel-5",
    "zofe/rapyd": "^2.1",
    "baum/baum": "^1.1",
    "sroutier/lesk-modules": "^3.0",
    "mgallegos/laravel-jqgrid": "^1.0",
    "creativeorange/gravatar": "^1.0",
    "tylercd100/lern": "^3.6.0",
    "arcanedev/settings": "^1.2",
    "panique/pdo-debug": "^0.2.0",
    "filp/whoops": "^1.0",
    "prettus/l5-repository": "^2.6",
    "bosnadev/repositories": "0.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "jeroen-g/laravel-packager": "dev-master",
    "libern/laravel-sql-logging": "^1.0@dev"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan ide-helper:generate",
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}